    "eslint-plugin-tailwindcss": "^3.17.4",
    "postcss": "^8.4.41",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "@types/bun": "^1.1.6"
  },
  "engines": {
    "node": ">=18"
